package cwst.com.entities;

import java.io.Serializable;
import javax.persistence.*;

/**
 * The primary key class for the CWST_SENTMAIL database table.
 * 
 */
@Embeddable
public class CwstSentmailPK implements Serializable {
	//default serial version id, required for serializable classes.
	private static final long serialVersionUID = 1L;

	@Column(name="CASE_ID", unique=true, nullable=false, length=50)
	private String caseId;

	@Column(unique=true, nullable=false, length=14)
	private String mailtype;

	public CwstSentmailPK() {
	}
	public String getCaseId() {
		return this.caseId;
	}
	public void setCaseId(String caseId) {
		this.caseId = caseId;
	}
	public String getMailtype() {
		return this.mailtype;
	}
	public void setMailtype(String mailtype) {
		this.mailtype = mailtype;
	}

	public boolean equals(Object other) {
		if (this == other) {
			return true;
		}
		if (!(other instanceof CwstSentmailPK)) {
			return false;
		}
		CwstSentmailPK castOther = (CwstSentmailPK)other;
		return 
			this.caseId.equals(castOther.caseId)
			&& this.mailtype.equals(castOther.mailtype);
	}

	public int hashCode() {
		final int prime = 31;
		int hash = 17;
		hash = hash * prime + this.caseId.hashCode();
		hash = hash * prime + this.mailtype.hashCode();
		
		return hash;
	}
}